<TITLE>12 sided calendar</TITLE>
<LINK REL="SHORTCUT ICON" HREF="pentagon.gif" TYPE="image/x-icon">

<SCRIPT TYPE="text/javascript" LANGUAGE="JavaScript">
function getCalendar(form) {
        Shape=document.calendar.shape.value;
        Year=document.calendar.year.value;
        Lang=document.calendar.language.value;
        Day=document.calendar.day.value;
        Format=document.calendar.format.value;
    
        URL = "http://folk.uib.no/nmioa/kalender/" + Year + "/" + Shape + "-" + Lang + "-" + Year + Day + "." + Format;

        location = URL;

        return true;
}
</SCRIPT>
